{
  "name": "@umich-michr/react-form-validation",
  "version": "0.1.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "description": "MICHR at the University of Michigan React Form Component Library to help with building react form components allowing for declarative and programmatic validations.",
  "private": true,
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "scripts": {
    "cypress:open": "npx cypress open",
    "clean": "npx shx rm -rf dist",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:cy:e2e": "npm start -- --no-open & wait-on https-get://localhost:5001 && npx cypress run",
    "test:cy:component": "npx cypress run --component",
    "start": "npx webpack-dev-server --config webpack.dev.config.js --open",
    "build:esm": "npx tsc --outDir dist/lib-es5 && npx tsc -m es6 --outDir dist/lib-es6",
    "build": "npm run prettier && npm run lintjs && npm run stylelint && npm run clean && npm run build:esm && webpack",
    "lintjs": "npx eslint \"main/**/*\"",
    "stylelint": "npx stylelint \"**/*.css\" \"**/*.scss\"",
    "prepare": "ts-patch install -s",
    "prettier": "npx prettier --write \"./main/**/*.{ts,tsx}\" \"./test/**/*.{ts,tsx}\""
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/umich-michr/react-form-validation.git"
  },
  "author": "MICHR Developers <michr-developers@umich.edu>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/umich-michr/react-form-validation/issues"
  },
  "homepage": "https://github.com/umich-michr/react-form-validation#readme",
  "keywords": [
    "react",
    "form",
    "validation"
  ],
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/eslint-plugin": "^7.19.1",
    "@babel/preset-env": "^7.19.4",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@types/mocha": "^10.0.0",
    "@types/node": "^18.11.0",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "babel-loader": "^8.2.5",
    "css-loader": "^6.7.1",
    "cypress": "^10.10.0",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-chai-friendly": "^0.7.2",
    "eslint-plugin-css": "^0.6.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-html": "^7.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "find-process": "^1.4.7",
    "html-webpack-plugin": "^5.5.0",
    "i": "^0.3.7",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-select": "^5.5.4",
    "sass": "^1.55.0",
    "sass-loader": "^13.1.0",
    "shx": "^0.3.4",
    "style-loader": "^3.3.1",
    "stylelint": "^14.14.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-prettier-scss": "^0.0.1",
    "stylelint-config-standard-scss": "^5.0.0",
    "stylelint-prettier": "^2.0.0",
    "stylelint-scss": "^4.3.0",
    "terser-webpack-plugin": "^5.3.6",
    "ts-loader": "^9.4.1",
    "ts-patch": "^2.0.2",
    "tsconfig-paths-webpack-plugin": "^4.0.0",
    "typescript": "^4.8.4",
    "typescript-transform-paths": "^3.3.1",
    "url-loader": "^4.1.1",
    "wait-on": "^6.0.1",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.1",
    "webpack-node-externals": "^3.0.0"
  }
}
